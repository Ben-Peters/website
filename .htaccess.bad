RewriteCond %{REQUEST_URI} !^(/|/index.html|/PubCookie.reply|/database/)$
RewriteCond %{REQUEST_URI} !^/?(database2|database|pages|Archive|Images|assets|cookies|css|databaseg|examples|headshots|images|jquery|js|newsite|public|rental_contracts|sharepoint|~lnl)/


RewriteRule ^(.*)$ mysite.fcgi/$1 [QSA,L]