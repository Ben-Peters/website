AddHandler fastcgi-script .fcgi
AddHandler cgi-script .py
RewriteEngine On

RewriteBase /

# RewriteRule ^(media/.*)$ - [L]

RewriteRule ^(media/.*)$ - [L]
RewriteRule ^(static/.*)$ - [L]
RewriteCond %{REQUEST_FILENAME} !-f

